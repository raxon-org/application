{
    "name" : "raxon/application",
    "description" : "raxon/application see https://raxon.org for detailed usage",
    "type" : "package",
    "keywords" : ["operation","microstorm", "micro", "storm","raxon", "org" ],
    "homepage": "https://raxon.org",
    "require": {
      "raxon/microstorm": "dev-main",
      "raxon/exception": "dev-main",
      "raxon/module": "dev-main",
      "raxon/router": "dev-main",
      "raxon/plugin": "dev-main",
      "latte/latte": "^3.1"
    },
    "autoload": {
      "psr-4": {
        "Application\\": "src/"
      }
    },
    "scripts": {
        "post-update-cmd": [
          "php -r \"copy('src/Binary', '/usr/bin/microstorm');\"",
          "chmod +x /usr/bin/microstorm",
          "microstorm boot -update"
        ]
    },
    "license" : "MIT",
    "authors" : [{
        "name" : "Remco van der Velde",
        "email" : "development@universeorange.com"
    }]
}
